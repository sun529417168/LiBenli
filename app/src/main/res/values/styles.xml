<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blue4</item>
        <item name="colorPrimaryDark">@color/blue4</item>
        <item name="colorAccent">@color/blue4</item>
    </style>

    <!--主界面底部的ImageView属性start-->
    <style name="main_image">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:clickable">false</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <!--主界面底部的ImageView属性end-->

    <!--主界面底部的TextView属性start-->
    <style name="main_textView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:textSize">10sp</item>
    </style>
    <!--主界面底部的TextView属性end-->

    <!--登录界面中间一条线begin-->
    <style name="viewline">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/ed_line</item>
    </style>
    <!--登录界面中间一条线end-->

    <!--标题栏左边返回按钮begin-->
    <style name="title_img_white">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@mipmap/back</item>
        <!--标题栏左边返回按钮end-->
    </style>

    <!--标题中间文本begin-->
    <style name="title_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!--标题中间文本begin-->

    <!--点名Item RelativeLayout begin-->
    <style name="rl_release_label">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/hr_rl_selected</item>
    </style>
    <!--点名Item RelativeLayout begin-->

    <!-- 点名Item TextView begin -->
    <style name="tv_release_lable">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center|left</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColorHint">@color/gray_text</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <!-- 点名Item TextView begin -->

    <!-- 点名Item 横线 begin -->
    <style name="view_release_viewline">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:background">@color/colorE2E2E2</item>
    </style>
    <!-- 点名Item 横线 end -->

    <!-- 点名界面 正常请假 begin -->
    <style name="task_button">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <!-- 点名界面 正常请假 end -->

    <!--成绩排名img begin-->
    <style name="notice_image">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">18dp</item>
    </style>
    <!--成绩排名img end-->

    <!--成绩排名 begin-->
    <style name="notice_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
    </style>
    <!--成绩排名 end-->

    <!--成绩排名Item begin-->
    <style name="notice_item_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textStyle">bold</item>
    </style>
    <!--成绩排名Item end-->

    <!--我 RelativeLayout begin-->
    <style name="mine_item_relativelayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/mine_item_rl</item>
    </style>
    <!--我 RelativeLayout end-->

    <!-- 我 Image begin -->
    <style name="me_img">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>
    <!-- 我 Image end -->

    <!--我 TextView begin-->
    <style name="me_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <!--我 TextView end-->

    <!--我 右边箭头 begin-->
    <style name="mine_arrow">
        <item name="android:layout_width">8dp</item>
        <item name="android:layout_height">14dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:background">@mipmap/arrow</item>
    </style>
    <!--我 右边箭头 end-->

    <!-- 我-横线 begin -->
    <style name="line_color_e2e2e2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:background">@color/colorE2E2E2</item>
    </style>
    <!-- 我-横线 begin -->
    =""
    android:layout_height=""
    android:gravity=""
    android:textColor=""
    android:textSize=""
    <!--我-修改道馆信息 TextView begin-->
    <style name="mine_updateinfo_textview">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14dp</item>
    </style>
    <!--我-修改道馆信息 TextView end-->

    <!--我-修改道馆信息 EditText begin-->
    <style name="mine_updateinfo_edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@null</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/gray_text3</item>
        <item name="android:textColorHint">@color/colorBBBBBB</item>
        <item name="android:textSize">14dp</item>
    </style>
    <!--我-修改道馆信息 EditText end-->

    <!--横线 begin-->
    <style name="line_color_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/gray</item>
    </style>
    <!--横线 end-->

    <!-- 设置对话框的透明 和点击外面无效果start -->
    <style name="DialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <!-- 设置对话框的透明 和点击外面无效果end -->
</resources>
